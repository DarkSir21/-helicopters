              - var n = 0;
                while n < 12
                  .col-xs-3
                    .card-hidden
                      .wrapper
                        a(href="#").image
                          img(src="images/helic.jpg")
                        a(href="#").title.-link Квадрокоптеры
                        .title.-small 1344 шт
                        .desc
                          a(href="#").tag Для новичков
                          a(href="#").tag Профессиональные
                          a(href="#").tag С камерой
                          a(href="#").tag DJI
                          a(href="#").tag Мультикоптеры
